{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Document De-Bundler",
  "version": "0.1.0",
  "identifier": "com.document-de-bundler.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:5173",
    "frontendDist": "../dist"
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "targets": "all",
    "resources": [],
    "externalBin": []
  },
  "app": {
    "withGlobalTauri": false,
    "windows": [
      {
        "title": "Document De-Bundler",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600,
        "resizable": true,
        "fullscreen": false,
        "label": "main"
      }
    ],
    "security": {
      "csp": null,
      "capabilities": ["default"]
    }
  },
  "plugins": {}
}
